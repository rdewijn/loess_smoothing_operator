{
  "name": "LOESS smoothing",
  "description": "Performs a loess curve fitting.",
  "tags": ["data visualisation", "smoothing", "modelling"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/loess_smoothing_operator"],
  "properties": [
    {
    "kind": "EnumeratedProperty",
    "name": "mode",
    "defaultValue": "smooth",
    "values": [
      "smooth",
      "regress"
    ],
    "description": "operator mode"
  },
    {
    "kind": "DoubleProperty",
    "name": "n_preds",
    "defaultValue": 1000,
    "description": "Number of points to generate predictions for (length or a regular sequence along x's range)."
    },
    {
    "kind": "DoubleProperty",
    "name": "span",
    "defaultValue": 0.75,
    "description": "Span parameter for loess smoothing (higher = smoother)."
    }  
  ]
}


